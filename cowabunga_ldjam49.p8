-- draw

function drawarena()
	palt(0,false)
	palt(4,false)
	cls(4)
	palt(4,true)
	_sky=3
	pal(11,_skys[_sky])
	local rx=0
	local ry=0
	local shk=mid(-8,_shak,8)
	if shk!=0 then
		rx=rnd(2*shk+1)-shk
		ry=rnd(2*shk+1)-shk
	end
	camera(_camx+rx,_camy+ry)
	-- draw background
	map(0,12,0,0,18,18)
	-- draw cow
	local f1=1
	local f2=17
	local x1=_cowx
	local x2=_cowx
	local y1=_cowy
	local y2=_cowy+8
	local flp=false
	local t1=_tmrs["walk"]
	if _cowdr=="lf" then
		flp=true
	end
	if not _cowup then
		f1=16
		f2=32
		x1=_cowx-4
		x2=_cowx+4
		if flp then
			x1+=8
			x2-=8
		end
		y1=_cowy+8
		y2=_cowy+8
	elseif t1>0 then
		f2+=flr((15+t1/15)%4)
	end
	if _tmrs["hurt"]%2==0 then
		spr(f1,x1,y1,1,1,flp)
		spr(f2,x2,y2,1,1,flp)
	end
	-- draw enemies
	for f in all(_foes) do
		local frm=_typs[f[1]][1]
		spr(frm,f[2],f[3],2,2,f[6])
	end
	-- draw attacks
	for a in all(_atks) do
		local aflp=false
		if a[4]<0 then
			aflp=true
		end
		local fm=3
		if a[1]==1 then
			fm=45
		end
		spr(fm,a[2],a[3],1,1,aflp)
	end
	-- draw city status bar
	print("city",9,121,7)
	local city=100*(_ctyhp/_maxhp)
	rectfill(27,120,129,126,14)
	rectfill(28,121,128,125,2)
	rectfill(28,121,28+city,125,8)
	-- draw milk status bar
	print("milk",9,129,7)
	local milk=100*(_cowhp/_maxhp)
	rectfill(27,128,129,134,6)
	rectfill(28,129,128,133,5)
	rectfill(28,129,28+milk,133,7)
	line(48,129,48,133,8)
	-- draw particles
	for p in all(_pxls) do
		pset(p[2],p[3],p[1])
	end
	local debug=_diff.." "
	debug=debug..#_foes.." "
	debug=debug.._tmrs["game"]
	print(debug,9,113,7)
end
