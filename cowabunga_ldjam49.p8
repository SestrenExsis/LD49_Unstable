-- intro

function initintro()
	_page=1
	_book=_books["intro"]
	initfn=initintro
	updatefn=updateintro
	drawfn=drawintro
	music(4,8000)
end

function updateintro()
	if btnp(‚ùé) or btnp(üÖæÔ∏è) then
		if _page>2 then
			initarena()
		end
	end
end

function drawintro()
	palt(0,false)
	palt(4,false)
	cls(4)
	palt(4,true)
	if t()<2 then
		_sky=1
		_page=4
	elseif t()<10 then
		_sky=1
		_page=1
	elseif t()<18 then
		_sky=2
		_page=2
	elseif t()<26 then
		_sky=3
		_page=3
	elseif t()<34 then
		_sky=3
		_page=4
	else
		_sky=3
		_page=4
	end
	pal(11,_skys[_sky])
	camera(_camx,_camy)
	-- draw background
	map(0,12,0,0,18,18)
	i=0
	for ln in all(_book[_page]) do
		print(ln,10,6*i+56,7)
		i+=1
	end
end
