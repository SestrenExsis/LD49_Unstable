-- main

function initarena()
	_cowhp=_bgnhp
	_cowup=true
	_cowx=60
	_cowy=_flor
	_cowdr="rt"
	_shak=0
	_tmrs["walk"]=0
	_tmrs["hurt"]=_invn
	_tmrs["game"]=3600
	initfn=initarena
	updatefn=updatearena
	drawfn=drawarena
	music(3,32000)
end

function dothecowthings()
	local mov=false
	local tx=_cowx
	local ty=_cowy
	if btn(‚û°Ô∏è) then
		mov=true
		tx+=1
		_cowdr="rt"
	elseif btn(‚¨ÖÔ∏è) then
		mov=true
		tx-=1
		_cowdr="lf"
	end
	if btnp(‚ùé) then
		local vx=2
		local vy=0
		if _cowdr=="lf" then
			vx*=-1
		end
		add(_atks,
			{0,_cowx,_cowy+2,vx,vy,30}
		)
		_cowhp-=_tols["fire"] or 0
		fx(
			_cowx,_cowy+2,2*vx,0,
			_tols["fire"]
		)
	end
	if btnp(üÖæÔ∏è) then
		_cowvy=_jump
		_cowhp-=_tols["jump"] or 0
		fx(
			_cowx,_cowy,0,2,
			_tols["jump"]
		)
	end
	if mov then
		_cowx=tx
		_cowy=ty
		_tmrs["walk"]+=1
	else
		_tmrs["walk"]=0
	end
end

function updatearena()
	if _cowup then
		dothecowthings()
	end
	for a in all(_atks) do
		local vx=a[4]
		local vy=a[5]
		a[2]+=vx
		a[3]+=vy
		a[6]-=1
		if a[6]<1 then
			del(_atks,a)
		end
	end
	-- apply jumping and gravity
	_cowy=mid(
		_ceil,
		_cowy+_cowvy,
		_flor
	)
	if _cowy==_flor then
		_cowvy=0
	end
	_cowvy+=_grav
	-- check if hit by attack
	local cx1=_cowx
	local cy1=_cowy
	local cx2=_cowx+8
	local cy2=_cowy+16
	local hitcow=false
	local hitcity=false
	for a in all(_atks) do
		local ax1=a[2]
		local ay1=a[3]
		local ax2=a[2]+8
		local ay2=a[3]+8
		if collides(
			a[1],ax1,ay1,ax2,ay2,
			0,cx1,cy1,cx2,cy2
		) then
			if (
				_cowup and
				_tmrs["hurt"]<1
			) then
				hitcow=true
				del(_atks,a)
			end
		elseif collides(
			a[1],ax1,ay1,ax2,ay2,
			0,64,96,80,112
		) then
			hitcity=true
		end
		if a[1]==0 then
			fx(ax1+4,ay1+6,0,1,1)
		end
	end
	-- check if cow hit by enemy
	for f in all(_foes) do
		local fx1=f[2]
		local fy1=f[3]
		local fx2=f[2]+16
		local fy2=f[3]+16
		-- enemy hits cow
		if collides(
			f[1],fx1,fy1,fx2,fy2,
			0,cx1,cy1,cx2,cy2
		) then
			if (
				_cowup and
				_tmrs["hurt"]<1
			) then
				hitcow=true
				del(_foes,f)
			end
		-- enemy hits heart of city
		elseif collides(
			f[1],fx1,fy1,fx2,fy2,
			0,64,96,80,112
		) then
			hitcity=true
		end
	end
	-- update health
	if hitcow then
		_cowhp-=_tols["hurt"]
		_tmrs["hurt"]=_invn
		fx(
			_cowx,_cowy,0,0,
			_tols["hurt"]
		)
	end
	if hitcity then
		_ctyhp-=2
		_shak=2
	end
	_shak=max(0,_shak-1)
	if _cowhp<1 then
		_cowup=false
	end
	_cowhp+=1
	if not _cowup then
		_cowhp+=1
	end
	_cowhp=mid(0,_cowhp,_maxhp)
	if _cowhp>=_minhp then
		if not _cowup then
			_tmrs["hurt"]=_invn
			_cowup=true
		end
	end
	_tmrs["hurt"]=max(
		0,
		_tmrs["hurt"]-1
	)
	-- spawn new enemies
	_diff=_base
	local tmr=_tmrs["game"]
	if tmr<1 then
		_diff*=0
	elseif tmr<300 then
		_diff*=5
	elseif tmr<900 then
		_diff*=3
	elseif tmr<1800 then
		_diff*=2
	end
	if #_foes<2 then
		_diff*=3
	elseif #_foes>5 then
		_diff*=0.5
	end
	if rnd()<_diff then
		local x=rnd({-4,135})
		local y=0
		local typ=rnd({1,1,1,2})
		local hp=1
		local tmr=rnd(20,40)
		local flp=false
		if x>64 then
			flp=true
		end
		if typ==1 then
			y=rnd({12,28,44,60,76})
		else
			y=rnd({2,18,34,50})
		end
		add(_foes,
			{typ,x,y,hp,tmr,flp}
		)
	end
	-- update enemies
	for f in all(_foes) do
		-- enemy collisions
		local fx1=f[2]
		local fy1=f[3]
		local fx2=f[2]+16
		local fy2=f[3]+16
		for a in all(_atks) do
			local ax1=a[2]
			local ay1=a[3]
			local ax2=a[2]+8
			local ay2=a[3]+8
			if collides(
				1,fx1,fy1,fx2,fy2,
				a[1],ax1,ay1,ax2,ay2
			) then
				f[4]-=1
			end
		end
		-- enemy movement
		if f[1]==2 then
			if f[3]>=_flor then
				f[3]=_flor
			elseif f[6] then
				f[2]-=1
			else
				f[2]+=1
			end
			if f[2]<-5 then
				f[6]=false
				f[1]=3
			elseif f[2]>136 then
				f[6]=true
				f[1]=3
			end
		else
			local spd=1
			if f[1]==3 then
				spd=2
			end
			if f[2]<64 then
				f[2]+=spd
			elseif f[2]>64 then
				f[2]-=spd
			end
			if f[3]<_flor then
				f[3]+=spd
			elseif f[3]>_flor then
				f[3]-=spd
			end
		end
		-- enemy health
		if f[4]<1 then
			del(_foes,f)
		end
		-- enemy timers
		f[5]-=1
		if f[5]<1 then
			if f[1]==2 then
				add(_atks,
					{1,f[2],f[3],0,1,150}
				)
			end
			f[5]=rnd(120,240)
		end
	end
	-- update particles
	for p in all(_pxls) do
		p[2]+=p[4]
		p[3]+=p[5]
		p[6]-=1
		if p[6]<1 then
			del(_pxls,p)
		end
	end
	_tmrs["game"]-=1
end
